{
  "name": "@garden/shared",
  "version": "1.0.0",
  "description": "",
  "types": "src/index.d.ts",
  "main": "dist/monitor.umd.js",
  "esnext": "dist/esnext/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "microbundle watch -i src/index.ts --external none --compress false -f umd --generateTypes false --sourcemap false --output examples/demo.umd.js --name Demo",
    "server": "http-server ./examples",
    "examples": "concurrently yarn:dev yarn:server",
    "build": "yarn build:cleanup && concurrently yarn:build:cjs yarn:build:esm yarn:build:esnext yarn:build:sdk --kill-others-on-fail",
    "build:cleanup": "rimraf ./dist/cjs ./dist/esm ./dist/esnext",
    "build:cjs": "ttsc -p ./tsconfig.build.json --module CommonJS --target ES5 --outDir ./dist/cjs",
    "build:esm": "ttsc -p ./tsconfig.build.json --module ES6 --target ES5 --outDir ./dist/esm",
    "build:esnext": "ttsc -p ./tsconfig.build.json --module ESNext --target ESNext --outDir ./dist/esnext",
    "build:sdk": "rollup --config"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.20.1",
    "microbundle": "^0.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-loader": "^9.0.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "http-server": "^14.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.26.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4"
  }
}
